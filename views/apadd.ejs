	<body class="cbp-spmenu-push">
		<div class="main-content">
		<div class="cbp-spmenu cbp-spmenu-vertical cbp-spmenu-left" id="cbp-spmenu-s1">
			<!--left-fixed -navigation-->
			<%- include('sidebar'); -%>
		</div>
			<!--left-fixed -navigation-->
			<%- include('headermenu'); -%>
			<!-- header-starts -->
		
			<!-- //header-ends -->
			<!-- main content start-->
			<div id="page-wrapper">
				<div class="main-page">
					<div class="forms">
						<h2 class="title1">AP -> 등록</h2>
						<div class="form-grids row widget-shadow" data-example-id="basic-forms"> 
							<div class="form-title">
								<h4>신규AP 등록</h4>
							</div>
							<div class="form-body">
						<form action="/apa" method="POST"> 
							<div class="form-group"> <label for="mac">AP Number</label> 
								<input type="text" class="form-control" id="apnum" placeholder="Enter AP #"> 
							</div> 
							<div class="form-group"> <label for="mac">AP MAC Address</label> 
								<input type="text" class="form-control" id="mac" placeholder="Enter MAC"> 
							</div> 
							<div class="form-group"> 
								<label for="ipadd">AP IP Address</label> 
								<input type="text" class="form-control" id="ip" placeholder="Enter IP"> 
							</div> 
							<div class="form-group"> 
								<label for="fwv">Firmware Verion</label> 
								<input type="text" class="form-control" id="fwv" placeholder="Enter Version"> 
							</div>
							<div class="form-group"> 
								<label for="idate">설치일</label> 
								<input type="text" class="form-control" id="idate" placeholder="Enter date"> 
							</div> 
							<div class="form-group"> 
								<label for="admin">설치장소</label> 
								<input type="text" class="form-control" id="location" placeholder="Enter 설치장소"> 
							</div> 
                            <div class="form-group"> 
								<label for="admin">담당자</label> 
								<input type="text" class="form-control" id="admin" placeholder="Enter 이름"> 
							</div> 

						<div class="form-group">
							</div> 
							<button type="submit" class="btn btn-default">등록</button> 
							
						</form> 
							</div>
						</div>
					</div>
				</div>
			</div>
			<!--footer-->
			<%- include('footer'); -%>
			<!--//footer-->
		</div>
		
		<!-- side nav js -->
		<script src='js/SidebarNav.min.js' type='text/javascript'></script>
		<script>
		$('.sidebar-menu').SidebarNav()
		</script>
		<!-- //side nav js -->
		
		<!-- Classie --><!-- for toggle left push menu script -->
			<script src="js/classie.js"></script>
			<script>
				var menuLeft = document.getElementById( 'cbp-spmenu-s1' ),
					showLeftPush = document.getElementById( 'showLeftPush' ),
					body = document.body;
					
				showLeftPush.onclick = function() {
					classie.toggle( this, 'active' );
					classie.toggle( body, 'cbp-spmenu-push-toright' );
					classie.toggle( menuLeft, 'cbp-spmenu-open' );
					disableOther( 'showLeftPush' );
				};
				
				function disableOther( button ) {
					if( button !== 'showLeftPush' ) {
						classie.toggle( showLeftPush, 'disabled' );
					}
				}
			</script>
		<!-- //Classie --><!-- //for toggle left push menu script -->
		
		<!--scrolling js-->
		<script src="js/jquery.nicescroll.js"></script>
